{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev:dashboard": "vite --clearScreen false",
    "dev": "concurrently --kill-others npm:dev:*",
    "dev:container": "concurrently npm:dev:server npm:dev:dashboard",
    "build": "tsc",
    "build:dashboard": "vite build",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*",
    "seed": "ts-node ./src/seed/seed.ts"
  },
  "dependencies": {
    "@vendure/asset-server-plugin": "3.5.4",
    "@vendure/core": "3.5.4",
    "@vendure/dashboard": "3.5.4",
    "@vendure/email-plugin": "3.5.4",
    "@vendure/graphiql-plugin": "3.5.4",
    "dotenv": "17.2.3",
    "pg": "8.16.3"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@vendure/cli": "^3.5.4",
    "concurrently": "9.2.1",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2"
  }
}
